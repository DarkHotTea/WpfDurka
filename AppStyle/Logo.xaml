<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="Second_label" TargetType="{x:Type Label}">
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Padding" Value="5"/>
        <Setter Property="Foreground" Value="#FF00E3FF"/>
        <Setter Property="Effect">
            <Setter.Value>
                <DropShadowEffect Color="#FF00E3FF" BlurRadius="4"  Direction="-50" ShadowDepth="1"/>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="First_label" TargetType="{x:Type Label}">
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Padding" Value="5,5,0,5"/>
        <Setter Property="FontWeight" Value="Bold"/>
    </Style>
    <Style x:Key="Logo_Menu" TargetType="{x:Type Border}">
        <Setter Property="Background" Value="DeepSkyBlue"/>
        <Setter Property="BorderThickness" Value="4.5"/>
        <Setter Property="CornerRadius" Value="50"/>
        <Setter Property="BorderBrush" Value="#FFA5F5FF"/>
    </Style>
</ResourceDictionary>